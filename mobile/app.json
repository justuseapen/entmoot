{
  "expo": {
    "name": "Entmoot",
    "slug": "entmoot",
    "version": "1.0.0",
    "description": "Family planning and goal tracking app. Plan your day, build habits, set SMART goals with AI coaching, and celebrate achievements together.",
    "privacy": "public",
    "githubUrl": "https://github.com/entmoot/entmoot-mobile",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "entmoot",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF8E7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.entmoot.mobile",
      "buildNumber": "1",
      "icon": "./assets/icon.png",
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"],
        "MinimumOSVersion": "14.0"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFF8E7"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "app.entmoot.mobile",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "releaseChannel": "development",
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#2D5A27",
          "sounds": [],
          "defaultChannel": "default"
        }
      ],
      "expo-router",
      "expo-localization"
    ]
  }
}
